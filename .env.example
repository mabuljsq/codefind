# CodeFind Development Environment Configuration
# Copy this file to .env and fill in your actual AWS credentials

# AWS Credentials (for development/testing only)
# In production, use IAM roles, AWS profiles, or other secure methods
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_SESSION_TOKEN=your_session_token_if_using_temporary_credentials
AWS_DEFAULT_REGION=us-west-2

# Optional: AWS Profile (alternative to access keys)
# AWS_PROFILE=your_aws_profile_name

# CodeFind Settings
CODEFIND_MODEL=bedrock/anthropic.claude-3-5-sonnet-20241022-v2:0
CODEFIND_AUTO_COMMITS=true
CODEFIND_VERBOSE=false

# Disable version checking and analytics (already removed from CodeFind)
CODEFIND_CHECK_UPDATE=false
